{
  "request": {
    "method": "POST",
    "url": "/api/price-strategy3",
    "headers": {
      "Content-Type": {
        "contains": "application/json"
      }
    }
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "price": "{{jsonPath request.body '$.base' multiply 1.10 add 5 round 2}}"
    },
    "headers": {
      "Content-Type": "application/json"
    },
    "transformers": ["response-template"]
  }
}
